{
  "$schema": "https://repomix.com/schemas/latest/schema.json",
  "output": {
    "filePath": "repomix-output.txt",
    "style": "plain"
  },
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": []
  },
  "include": ["{scripts,src,tests}/**"],
  "security": {
    "enableSecurityCheck": true
  }
}
